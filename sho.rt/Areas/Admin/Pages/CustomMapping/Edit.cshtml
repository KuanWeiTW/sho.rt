@page
@model sho.rt.Areas.Admin.Pages.CustomMapping.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>CustomMapping</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CustomMapping.Id" />
            <div class="form-group">
                <label asp-for="CustomMapping.Password" class="control-label"></label>
                <input asp-for="CustomMapping.Password" class="form-control" />
                <span asp-validation-for="CustomMapping.Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CustomMapping.Original" class="control-label"></label>
                <input asp-for="CustomMapping.Original" class="form-control" />
                <span asp-validation-for="CustomMapping.Original" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CustomMapping.OwnerId" class="control-label"></label>
                <select asp-for="CustomMapping.OwnerId" class="form-control" asp-items="ViewBag.Owner"></select>
                <span asp-validation-for="CustomMapping.OwnerId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
